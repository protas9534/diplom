.catalog 
    .container.catalog__container 
        .catalog__top 
            .catalog__breadcrumb 
                img(src="assets/imgs/diamond.svg", alt="")
                .breadcrumb__text Ювелирные украшения / Кольца
            .catalog__title Кольца
            .catalog__filter 
                .catalog__filter-item 
                    .catalog__filter-title Категория
                    p.catalog__filter-radio
                        .radio
                            <input class="radio__input" type="radio" name="category" id="a1" checked>
                            <label for="a1"  class="radio__label"> Кольца
                        .radio
                            <input class="radio__input" type="radio" name="category" id="a2">
                            <label for="a2"  class="radio__label"> Серьги
                        .radio
                            <input class="radio__input" type="radio" name="category" id="a3">
                            <label for="a3"  class="radio__label"> Подвески
                .catalog__filter-item 
                    .catalog__filter-title Материал
                    p.catalog__filter-radio
                        .radio
                            <input class="radio__input" type="radio" name="material" id="a4" checked>
                            <label for="a4"  class="radio__label"> Золото
                        .radio
                            <input class="radio__input" type="radio" name="material" id="a5">
                            <label for="a5"  class="radio__label"> Серебро
                .catalog__filter-item 
                    .catalog__filter-title Вставка
                    p.catalog__filter-radio
                        .radio
                            <input class="radio__input" type="radio" name="insert" id="a6" checked>
                            <label for="a6"  class="radio__label"> Бриллиант
                        .radio
                            <input class="radio__input" type="radio" name="insert" id="a7">
                            <label for="a7"  class="radio__label"> Сапфир
                        .radio
                            <input class="radio__input" type="radio" name="insert" id="a8">
                            <label for="a8"  class="radio__label"> Изумруд
                        .radio
                            <input class="radio__input" type="radio" name="insert" id="a9">
                            <label for="a9"  class="radio__label"> Рубин
                        .radio
                            <input class="radio__input" type="radio" name="insert" id="a10">
                            <label for="a10"  class="radio__label"> Топаз
                .catalog__filter-item 
                    .catalog__filter-title Проба
                    p.catalog__filter-radio
                        .radio
                            <input class="radio__input" type="radio" name="content" id="a11">
                            <label for="a11"  class="radio__label"> 585
                        .radio
                            <input class="radio__input" type="radio" name="content" id="a12" checked>
                            <label for="a12" class="radio__label"> 750
                        .radio
                            <input class="radio__input" type="radio" name="content" id="a13">
                            <label for="a13" class="radio__label"> 925
                .catalog__filter-item 
                    .catalog__filter-title Наличие
                    p.catalog__filter-radio
                        .radio
                            <input class="radio__input" type="radio" name="availability" id="a14"> 
                            <label for="a14" class="radio__label"> Есть в наличии
            .catalog__sort
                .catalog__sort-title Сортировать по:
                .catalog__sort-item Популярности
                .catalog__sort-item По новизне
                .catalog__sort-item По цене
            .catalog__galery
                mixin product(vimg, vprice, voldprice, vtext, vtext_h, ratingTrue1, ratingTrue2, ratingTrue3, ratingTrue4, ratingTrue5)
                    .galery__col 
                        a(href="").product
                            .product__img 
                                img(src=vimg, alt="product")
                            .product__price=vprice
                            .product__old-price=voldprice
                                .product__rating
                                    .rating__item
                                        - if(ratingTrue1)
                                            img(src="./assets/imgs/star1.svg", alt="") 
                                        - else 
                                            img(src="./assets/imgs/star0.svg", alt="")
                                    .rating__item
                                        - if(ratingTrue2)
                                            img(src="./assets/imgs/star1.svg", alt="") 
                                        - else 
                                            img(src="./assets/imgs/star0.svg", alt="")
                                    .rating__item
                                        - if(ratingTrue3)
                                            img(src="./assets/imgs/star1.svg", alt="") 
                                        - else 
                                            img(src="./assets/imgs/star0.svg", alt="")
                                    .rating__item
                                        - if(ratingTrue4)
                                            img(src="./assets/imgs/star1.svg", alt="") 
                                        - else 
                                            img(src="./assets/imgs/star0.svg", alt="")
                                    .rating__item
                                        - if(ratingTrue5)
                                            img(src="./assets/imgs/star1.svg", alt="") 
                                        - else 
                                            img(src="./assets/imgs/star0.svg", alt="")
                            .product__text=vtext
                                .product__text_h=vtext_h
                            .product__btn в корзину
                +product('./assets/imgs/ring8.png','10 250 ₽',' ','Кольцо из комбинированного золота с бриллиантами','Желтое золото, 585° Вставки: бриллианты Вес: 2,27 г', true, true, true, true, false)
                +product('./assets/imgs/ring7.png','9 250 ₽','12 500 ₽','Кольцо из золота с родированием с бриллиантом','Желтое золото, 585° Вставки: бриллианты Вес: 2,31 г', true, true, true, false, false)            
                +product('./assets/imgs/ring6.png','18 750 ₽','27 000 ₽','Кольцо из белого золота 585 пробы с бриллиантами','Белое золото, 585° Вставки: бриллианты Вес: 1,85 г', true, true, true, true, true)
                +product('./assets/imgs/ring5.png','20 450 ₽','36 450 ₽','Помолвочное кольцо из белого золота с бриллиантом','Белое золото, 585° Вставки: бриллианты Вес: 0,94 г', true, true, true, true, true)            
                +product('./assets/imgs/ring6.png','18 750 ₽','27 000 ₽','Кольцо из белого золота 585 пробы с бриллиантами','Белое золото, 585° Вставки: бриллианты Вес: 1,85 г', true, true, true, true, true)                
                +product('./assets/imgs/ring5.png','20 450 ₽','36 450 ₽','Помолвочное кольцо из белого золота с бриллиантом','Белое золото, 585° Вставки: бриллианты Вес: 0,94 г', true, true, true, true, true)
                +product('./assets/imgs/ring8.png','10 250 ₽',' ','Кольцо из комбинированного золота с бриллиантами','Желтое золото, 585° Вставки: бриллианты Вес: 2,27 г', true, true, true, true, false)                
                +product('./assets/imgs/ring7.png','9 250 ₽','12 500 ₽','Кольцо из золота с родированием с бриллиантом','Желтое золото, 585° Вставки: бриллианты Вес: 2,31 г', true, true, true, false, false) 
